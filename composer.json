    "ext-json": "*",
    "ext-mysqli": "*",
    "ext-pdo": "*",
    "ext-simplexml": "*",
    "fzaninotto/faker": "1.*",
    "phing/phing": "2.*",
    "phpmailer/phpmailer": "6.*",
    "phpunit/phpunit": "7.*",
    "symfony/console": "4.*",
    "spatie/ssl-certificate": "1.*",
    "twig/twig": "2.*"
  }
}
