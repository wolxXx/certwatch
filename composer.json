{
  "name": "wolxxx/certwatch",
  "description": "observe the validity of your ssl certificates",
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
    {
      "name": "wolxXx",
      "email": "devops@wolxxx.de"
    }
  ],
  "support": {
    "email": "devops@wolxxx.de"
  },
  "config": {
    "secure-http": false
  },
  "autoload": {
    "psr-4": {
      "Certwatch\\": "src/"
    }
  },
  "require": {
    "php": ">=7.1.0",
    "ext-curl": "*",
    "ext-dom": "*",
    "ext-imap": "*",
